# Database Configuration
# PostgreSQL connection string (Neon, Supabase, ou outro)
DATABASE_URL="postgresql://user:password@host:5432/database"

# Exemplos:
# Neon: postgresql://user:pass@ep-xxx.neon.tech/dbname
# Supabase: postgresql://postgres:pass@db.xxx.supabase.co:5432/postgres
# Local: postgresql://postgres:postgres@localhost:5432/saas_core

# JWT Secret (m√≠nimo 32 caracteres)
JWT_SECRET="your-secret-key-change-in-production-min-32-chars"

# API Google Distance Matrix
GOOGLE_DISTANCE_MATRIX_API_KEY="sua-chave-api-aqui"

# API Google Maps (opcional, para mapas)
GOOGLE_MAPS_API_KEY="sua-chave-maps-aqui"

# Map ID do Google Maps (opcional)
GOOGLE_MAPS_MAP_ID="seu-map-id-aqui"